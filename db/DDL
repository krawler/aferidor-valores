
INSERT INTO AFERIDOR.CONSULTAS(ID, NOME, SQL_CONSULTA, TIPO_CONSULTA) VALUES(2, 'COMISSÃO NOTAS SAÍDA', 'select  p.nome, sum(nfc.vl_comissao) from nfsaida_comissao nfc inner join nfsaida nf on nf.codigo = nfc.idsaida and nf.idvendedor = nfc.idcolaborador and nf.idvendedor_loja = nfc.idcolaborador_loja inner join tipo_operacao op on op.codigo = nf.idoperacao and op.gera_comissao = -1  inner join pessoa_vendedor pv on pv.idpessoa = nfc.idcolaborador and pv.idloja = nfc.idcolaborador_loja inner join pessoa p on p.codigo = pv.idpessoa and p.loja = pv.idloja where nf.status in (1,2) and nf.idempresa = ? and nf.idempresa_loja = ?',
																 'TRANSACIONAL'); 
